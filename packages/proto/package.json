{
  "name": "@goat-rpc/proto",
  "version": "0.0.1",
  "description": "A protocol over WebSocket",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": null
    }
  },
  "scripts": {
    "build": "mkdir -p dist/src && pbjs -t static-module -w commonjs --es6 -o dist/src/generated.js res/goat_rpc.proto && pbts -o dist/src/generated.d.ts dist/src/generated.js",
    "clean": "scripts/common/clean.mjs",
    "start:dev": "scripts/start-dev.mjs"
  },
  "dependencies": {
    "protobufjs": "7.4.0",
    "protobufjs-cli": "1.1.3"
  }
}
