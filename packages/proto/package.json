{
  "name": "@goat-rpc/proto",
  "version": "0.0.1",
  "description": "A protocol over WebSocket",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsx scripts/build.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "protobufjs": "7.4.0"
  },
  "devDependencies": {
    "protobufjs-cli": "1.1.3"
  }
}
