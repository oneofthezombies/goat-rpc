{
  "name": "@goat-rpc/protocol",
  "version": "0.0.1",
  "description": "A RPC protocol over WebSocket",
  "keywords": [
    "grpc",
    "protocol"
  ],
  "license": "MIT",
  "author": {
    "name": "oneofthezombies",
    "email": "hunhoekim@gmail.com",
