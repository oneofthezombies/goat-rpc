{
  "name": "@goat-rpc/server",
  "version": "0.0.1",
  "description": "A modern TypeScript gRPC server framework",
  "keywords": [
    "grpc",
    "server"
  ],
  "license": "MIT",
  "author": {
    "name": "oneofthezombies",
    "email": "hunhoekim@gmail.com",
    "url": "https://github.com/oneofthezombies"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "check": "scripts/check.mjs",
    "dev": "scripts/dev.mjs",
    "start:dev": "tsx src/index.ts"
  },
  "dependencies": {
    "@goat-rpc/proto": "*"
  }
}
