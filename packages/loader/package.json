{
  "name": "@goat-rpc/loader",
  "version": "0.0.1",
  "description": "A modern TypeScript gRPC loader",
  "keywords": [
    "grpc",
    "loader"
  ],
  "license": "MIT",
  "author": {
    "name": "oneofthezombies",
    "email": "hunhoekim@gmail.com",
    "url": "https://github.com/oneofthezombies"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": null
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "scripts/build.mjs",
    "check": "scripts/check.mjs",
    "clean": "scripts/clean.mjs",
    "dev": "scripts/dev.mjs",
    "start:dev": "scripts/start-dev.mjs",
    "generate": "pbjs -t static-module -w es6 -o goat-rpc.js src/goat_rpc.proto && pbts -o goat-rpc.d.ts goat-rpc.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.11.3",
    "protobufjs": "7.4.0",
    "protobufjs-cli": "1.1.3"
  }
}
