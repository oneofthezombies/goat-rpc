{
  "name": "@goat-rpc/generator",
  "version": "0.0.1",
  "description": "A modern TypeScript gRPC generator",
  "keywords": [
    "grpc",
    "generator"
  ],
  "license": "MIT",
  "author": {
    "name": "oneofthezombies",
    "email": "hunhoekim@gmail.com",
    "url": "https://github.com/oneofthezombies"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": null
    }
  },
  "bin": {
    "goat-rpc-gen": "bin/goat-rpc-gen.js"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "node scripts/build.mjs",
    "check": "node scripts/check.mjs",
    "clean": "node scripts/clean.mjs",
    "dev": "node scripts/dev.mjs",
    "start:dev": "node scripts/start-dev.mjs"
  },
  "nodemonConfig": {
    "delay": "500",
    "exec": "node scripts/nodemon-exec.mjs",
    "ext": "ts"
  }
}
